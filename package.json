  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "cross-env NODE_ENV=development nodemon index.js",
    "dev:testdb": "cross-env NODE_ENV=test nodemon index.js",
    "test": "cross-env  NODE_ENV=test node --test",
    "lint": "eslint .",
    "start:test": "NODE_ENV=test node index.js",
    "build:ui": "rm -rf ./dist && cd ./frontend && npm run build && mv ./dist ../",
    "build:ui:win": "powershell -Command \"Remove-Item -Recurse -Force ./dist -ErrorAction SilentlyContinue; Push-Location ./frontend; npm run build; Pop-Location; Move-Item -Force ./frontend/dist ./\"",
    "deploy:full": "npm run build:ui:win && git add . && git commit -m 'uibuild' && git push"
  },
